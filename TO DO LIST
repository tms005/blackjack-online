void czyscBuffer(Buffer tempbuff)
{
    tempbuff.ID=0;
    tempbuff.ID_USR=0;
    for(int i=0;i<16;i++) tempbuff.iKey[i]=0;
    memset(tempbuff.cChat,0,256);
}

